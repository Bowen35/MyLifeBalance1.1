<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.inducesmile.androidmultiquiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuizMenuActivity" />
        <activity android:name=".QuizInstructionActivity" />
        <activity android:name=".BestScoreActivity" />
        <activity android:name=".QuizCategoryActivity" />
        <activity android:name=".MainQuizActivity" />
        <activity android:name=".QuizResultActivity" />
        <activity android:name=".ResultAnalysisActivity"></activity>
    </application>

</manifest>